{
  "cli": {
    "version": ">= 16.14.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "production": {
      "autoIncrement": false,
      "channel": "production",
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      }
    },
    "demo": {
      "autoIncrement": false,
      "channel": "demo",
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_WEB_APP_URL": "https://cool-cars-garage.co.uk",
        "EXPO_PUBLIC_API_URL": "https://api.cool-cars-garage.co.uk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
