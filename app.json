{
  "expo": {
    "name": "Cool Cars Reports",
    "slug": "cool-cars-reports",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "coolcarsreports",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundlerIdentifier": "com.coolcars.reports",
      "bundleIdentifier": "cool.cars.reports",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#007bff"
      },
      "edgeToEdgeEnabled": true,
      "package": "cool.cars.reports"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38636129-f85c-4650-a57a-a0958896054c"
      }
    },
    "runtimeVersion": "1.0.0-demo",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "enabled": true,
      "url": "https://u.expo.dev/38636129-f85c-4650-a57a-a0958896054c"
    },
    "owner": "marcucosmin3"
  }
}
